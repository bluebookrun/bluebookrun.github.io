<body>
  <header class="bb-navbar navbar navbar-static-top bb-navbar-landing" id="top">
  <div class="container">
    <div class="navbar-header">
      <button aria-controls="bb-navbar" aria-expanded="false" class="collapsed navbar-toggle" data-target="#bb-navbar" data-toggle="collapse" type="button">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a href="/" class="navbar-brand">Bluebook</a>
    </div>
    <nav class="collapse navbar-collapse" id="bb-navbar">
      <ul class="nav navbar-nav">
        <li><a href="/docs/getting-started">Getting Started</a></li>
        <li><a href="/docs/">Documentation</a></li>
        <li><a href="https://github.com/bluebookrun/bluebook">GitHub</a></li>
      </ul>
    </nav>
  </div>
</header>

  <div class="container">
    variable "server" {
    default = "http://localhost"
}

resource "http_assertion" "status_200" {
    source = "status_code"
    comparison = "equals"
    target = "200"
}

resource "http_step" "my_request" {
    method = "GET"
    url = "${var.server}/ping"

    assertions = [
        "${http_assertion.status_200.id}",
    ]
}

resource "http_test" "my_test" {
    steps = [
        "${http_step.my_request.id}",
    ]
}
  </div>
